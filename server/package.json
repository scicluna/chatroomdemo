    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.0"
  },
  "dependencies": {
    "@prisma/client": "^4.13.0",
    "express": "^4.18.2"
  }
}